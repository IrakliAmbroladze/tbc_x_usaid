{
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@/styles/*": ["styles/*"],
      "@/components/*": ["components/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "./dist/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/app/[locale]/(authUserView)/error.jsx", "src/app/components/AuthUserView.tsx", "src/app/components/UnAuthUserAlert.tsx", "src/app/[locale]/(authUserView)/loading.js", "src/app/api/auth/[auth0]/route.js", "src/app/api/products/route.js", "src/app/components/ThemeToggle.js", "src/app/components/language.js"  ],
  "exclude": [
    "node_modules"
  ]
}
